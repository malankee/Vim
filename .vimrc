set nocompatible              " be iMproved, required
filetype off                  " required

filetype plugin indent on    " required

set number
set relativenumber
syntax on
set wildmenu
set showcmd
set backspace=indent,eol,start
set confirm
set mouse=a

set smartindent
set tabstop=4
set shiftwidth=4
set expandtab

inoremap {<cr> {<cr>}<c-o><s-o>
inoremap [<cr> [<cr>]<c-o><s-o>
inoremap (<cr> (<cr>)<c-o><s-o>


